<html>
<head>
{% include '?trackingHeadStart' %}
{% include 'includes/head' %}
{% include 'includes/styles' %}
{% include '?trackingHeadEnd' %}
</head>
<body class="{% block bodyClass %}{% endblock %}">
{% include '?trackingBodyStart' %}
<!-- Wrapper -->
<div id="wrapper">

    <!-- Header -->
    <header id="header">
        <h1><a href="{{ site.baseUrl }}">{{ site.about.siteName }}</a></h1>
        <nav class="links">
            <ul>
            {% for menuItem in theme.topMenu.getMenuItems(page) %}
                <li><a href="{{ menuItem.link }}">{{menuItem.title}}</a></li>
            {% endfor %}
            </ul>
        </nav>
        <nav class="main">
            <ul>
                <li class="search">
                    <a class="fa-search" href="#search">Search</a>
                    <form id="search" method="get" action="#">
                        <input type="text" name="query" placeholder="Search"/>
                    </form>
                </li>
                <li class="menu">
                    <a class="fa-bars" href="#menu">Menu</a>
                </li>
            </ul>
        </nav>
    </header>

    <!-- Menu -->
    <section id="menu">

        <!-- Search -->
        <section>
            <form class="search" method="get" action="#">
                <input type="text" name="query" placeholder="Search"/>
            </form>
        </section>

        <!-- Links -->
        <section>
            <ul class="links">
            {% for menuItem in theme.drawerMenu.getMenuItems(page) %}
                <li>
                    <a href="{{ menuItem.link }}">
                        <h3>{{menuItem.title}}</h3>
                    </a>
                </li>
            {% endfor %}
            </ul>
        </section>

        <!-- Actions -->
        <section>
            <ul class="actions vertical">
                <li><a href="#" class="button big fit">Log In</a></li>
            </ul>
        </section>

    </section>

    {% block body %}{% endblock %}

</div>

{% include 'includes/scripts' %}

{% include '?trackingBodyEnd' %}
</body>
</html>